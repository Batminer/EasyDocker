# APPS - NEEDED FOR THE INSTALLATION OF APPS
#
# APP_NAME = name of the application for use in the scripts
# HOST NAME = subdomain name e.g test is the name for test.website.com
# DOMAIN NUMBER = number of the domain from the general config, useful when using multiple domains
# PUBLIC = if true, application will be setup with the reverse proxy and accessible from the internet
# PORT = port number of the application that will be used
#
#----------------------------------------------------------------
# PRIVACY
#----------------------------------------------------------------

# Mailcow
CFG_MAILCOW_APP_NAME=mailcow
CFG_MAILCOW_HOST_NAME=mail
CFG_MAILCOW_DOMAIN_NUMBER=1
CFG_MAILCOW_PUBLIC=true
CFG_MAILCOW_EASY_SETUP=true
CFG_MAILCOW_USING_CADDY=false

# Trilium
CFG_TRILIUM_APP_NAME=trilium
CFG_TRILIUM_HOST_NAME=notes
CFG_TRILIUM_DOMAIN_NUMBER=1
CFG_TRILIUM_PUBLIC=false
CFG_TRILIUM_PORT=8091

# IpInfo
CFG_IPINFO_APP_NAME=ipinfo
CFG_IPINFO_HOST_NAME=ipinfo
CFG_IPINFO_DOMAIN_NUMBER=1
CFG_IPINFO_PUBLIC=true
CFG_IPINFO_PORT=4545

# SearXNG
CFG_SEARXNG_APP_NAME=searxng
CFG_SEARXNG_HOST_NAME=search
CFG_SEARXNG_DOMAIN_NUMBER=1
CFG_SEARXNG_PUBLIC=true
CFG_SEARXNG_PORT=8080

# Speedtest
CFG_SPEEDTEST_APP_NAME=speedtest
CFG_SPEEDTEST_HOST_NAME=speedtest
CFG_SPEEDTEST_DOMAIN_NUMBER=1
CFG_SPEEDTEST_PUBLIC=true
CFG_SPEEDTEST_PORT=4001

# VaultWarden
CFG_VAULTWARDEN_APP_NAME=vaultwarden
CFG_VAULTWARDEN_HOST_NAME=vaultwarden
CFG_VAULTWARDEN_DOMAIN_NUMBER=1
CFG_VAULTWARDEN_PUBLIC=true
CFG_VAULTWARDEN_PORT=8201

# Actual
CFG_ACTUAL_APP_NAME=actual
CFG_ACTUAL_HOST_NAME=budget
CFG_ACTUAL_DOMAIN_NUMBER=1
